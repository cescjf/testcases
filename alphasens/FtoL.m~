clear
close all
clc

dF(1)=-0.547
dF(2)=9.11
dF(3)=0.0

F(1)=1.55175
F(2)=53.696
F(3)=0.0


% 
alpha=0.0
beta=6




sin_a = sind(alpha);
cos_a = cosd(alpha);
sin_b = sind(beta);
cos_b = cosd(beta);

  dL(1) =  dF(1)*cos_a*cos_b - dF(2)*cos_a*sin_b + dF(3)*sin_a;
  dL(2) =  dF(1)*sin_b       + dF(2)*cos_b;
  dL(3) = -dF(1)*sin_a*cos_b + dF(2)*sin_a*sin_b + dF(3)*cos_a;


  dL(1) = dL(1) F(1)*-cos_a*sin_b  + F(2)*-cos_a*cos_b + F(3)*0.0  ;
	  dL(2) += F(1)*cos_b         + F(2)*-sin_b       + F(3)*0.0  ;
	  dL(3) += F(1)*sin_a*sin_b   + F(2)*sin_a*cos_b  + F(3)*0.0  ;